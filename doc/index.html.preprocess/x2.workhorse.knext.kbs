	;; AN ATTACHMENT_ASSOCIATION_DESCRIPTION[PERSON??] CAN BE TO AN EXEMPLAR_OF A TASK[PERSON??].

;;   F

;;    S

		(and (ATTACHMENT_5FASSOCIATION_5FDESCRIPTION_27 ?x1) (PERSON_27 ?x2))

;;   F

;;    S

		(and (be_27 ?e1 ?x2) (to_27 ?e1 ?x1) (AN_27 ?x1) (EXEMPLAR_5FOF_27 ?x1) (A_27 ?x1) (TASK_27 ?x1) (person_27 ?x2))

;;  K

	;; AN EXEMPLAR_OF A TASK[PERSON??] CAN BE ASSOCIATED.

;;   F

;;    S

		(and (EXEMPLAR_5FOF_27 ?x1) (A_27 ?x1) (TASK_27 ?x1) (person_27 ?x2))

;;   F

;;    S

		(and (be_27 ?e1 ?x1) (ASSOCIATED_27 ?x1))


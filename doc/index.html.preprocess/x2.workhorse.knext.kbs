		(and (property_27 ?x1) (person_27 ?x2))

;;   F

;;    S

		(and (MAY_27 ?x1) (connect_27 ?e1 ?x1 ?x2) (AN_27 ?x2) (ATTACHMENT_5FASSOCIATION_5FDESCRIPTION_27 ?x2) (PERSON_27 ?x3))

;;   F


		(and (GENERATION_27 ?x1) (of_27 ?x1 ?x2) (IDENTIFIERS_27 ?x2) (CAN_27 ?x2) (BE_27 ?x2) (ARCHITECTURE_27 ?x2) (ISSUE_27 ?x2))

